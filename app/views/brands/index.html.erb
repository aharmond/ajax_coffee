<h2 class="center">Brands</h2>
<hr />
<div class="row">
  <div class="col s12 m6">
    <button class="btn" id="toggle">Add Brand<button>
    <ul id="brands-list">
      <% @brands.each do |brand| %>
        <%= render partial: 'brand', locals: { brand: brand } %>
      <% end %>
    </ul>
  </div>
  <div class="row" id="blends-list">
    <h3 id="brand">Blends</h3>
    <ul id="blends">
    </ul>
  </div>
</div>
